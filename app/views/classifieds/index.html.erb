
<%= stylesheet_link_tag "application", "jquery-ui" %>  
<%= javascript_include_tag :defaults, "jquery-ui.min", "autocomplete-rails" %>
  <%= render partial: "layouts/navbar" %>
<% if user_signed_in? %>
  Logged in as <strong><%= current_user.email %></strong>.
  <%= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'  %>
<% end %>

<div id="masonry-container" class="centered "> 
   
       <% @classifieds.each do |pin| %>   
         <div class="box col4"> 
         <p> <%= link_to image_tag(pin.image_url) %> </p>  
     <div class="post-info">
			        		<div class="post-basic-info">
				        		<p><h3><label><%= pin.title %></h3></p>
			        		</div>
			        		
			        	</div>                    
       </div> 
  
<% end %>     

</div>
